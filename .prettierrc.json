{
    "$schema": "http://json.schemastore.org/prettierrc",
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "all",
    "quoteProps": "consistent",
    "overrides": [
        {
            "files": "*.js",
            "options": {
                "trailingComma": "es5"
            }
        }
    ]
}
